#include "reactionRates.h"

#include <math.h>
#include <stdio.h>
#include "../Constants/programConstants.h"
#include "../Constants/physicalConstants.h"

void rate_weak(double f[],double r[])
/* calculates the nuclear forward rates of weak interaction reactions */
{
	int ie;
	
	f[1]=1.78141141239e-9;	/* H3 -> e- + v + He3 */
	f[2]=0.827;	 	/* Li8 -> e- + v + 2He4 */
	f[3]=34.3;	 	/* B12 -> e- + B + C12 */
	f[4]=3.834e-12;	 	/* C14 -> e- + v + N14 */
	f[5]=0.9;	 	/* B8 -> e+ + v + 2He4 */
	f[6]=5.668e-4;	 	/* C11 -> e+ + v + B11 */
	f[7]=63.01;	 	/* N12 -> e+ + v + C12 */
	f[8]=0.001159;	 	/* N13 -> e+ + v + C13 */
	f[9]=0.0098171;	 	/* O14 -> e+ + v + N14 */
	f[10]=0.0056704; 	/* O15 -> e+ + v + N15 */

	for(ie=1;ie<11;ie++)
	  r[ie]=0;

	return;
}

/*----------------------------------------------------*/

void rate_pn(double T9,double f[], double r[])
/* calculates the nuclear forward and reverse rates f[] and r[] of the reaction p <-> n at the temperature T9 */
{
	int ie;
	double z=5.929862032115561/T9;
	
	double a[14]={0.,0.15735,0.46172e1,-0.40520e2,0.13875e3,-0.59898e2,0.66752e2,-0.16705e2,0.38071e1,-0.39140,0.23590e-1,-0.83696e-4,-0.42095e-4,0.17675e-5};
	
	double b[11]={0.,0.22211e2,-0.72798e2,0.11571e3,-0.11763e2,0.45521e2,-0.37973e1,0.41266e0,-0.26210e-1,0.87934e-3,-0.12016e-4};
	
	f[0]=1.;
	for(ie=1;ie<=13;ie++) f[0]+=a[ie]/pow(z,ie);
	f[0]*=exp(-0.33979/z)/tau; /* n->p */
		
	if(z<5.10998997931)
	{
		r[0]=-0.62173;
		for(ie=1;ie<=10;ie++) r[0]+=b[ie]/pow(z,ie);
		r[0]*=exp(-2.8602*z)/tau; /* p->n */
	}
	else r[0]=0.; /* p->n */
		
	return;
}

/*----------------------------------------------------*/

void rate_all(double T9,double f[])
/* calculates the nuclear forward rates f[] of the nuclear reactions given below at the temperature T9 */
{
  int ie;

  /* H + n -> g + H2 */
  if(T9<1.5) f[11]=44060.*(1.-2.7503695564153143*T9-3.5220409333117897*T9*T9-0.2093513619089196*T9*T9*T9+0.10659679579058313*sqrt(T9)+ 4.62948586627009*pow(T9,3./2.) +1.3459574632779876*pow(T9,5./2.));
  else
    f[11]=(1.-sqrt(T9)*0.8504+T9*0.4895-pow(T9,1.5)*0.09623+T9*0.008471*T9-T9*2.8e-4*pow(T9,1.5))*47420.;
  //if(T9>1)printf("T9=%e\tp+n->d rate = %e\n",T9,f[11]);
	
  /* H2 + n -> g + H3 */
  f[12]=(T9*18.9+1.)*66.2;
	
  /* He3 + n -> g + He4 */
  f[13]=(T9*905.+1.)*6.62;
	
  /* Li6 + n -> g + Li7 */
  f[14]=5100.;
	
  /* He3 + n -> p + H3 */
  if(T9<2.5) f[15]=7.064935e8+6.733213571736319e8*T9+1.7181155480346258e9*T9*T9-4.5367658146835446e8*T9*T9*T9-1.2216728981712557e8*pow(T9,4.)-4.92736677238425e8*sqrt(T9)-1.3659670893994067e9*pow(T9,3./2.)-6.629932739639357e8*pow(T9,5./2.)+4.834951929033479e8*pow(T9,7./2.);
  else f[15]=4.81732e8;
	
  /* Be7 + n -> p + Li7 */
  if(T9<2.5) f[16]=6.8423032e9+1.7674863e10*T9+2.6622006e9*T9*T9-3.3561608e8*T9*T9*T9-5.9309139e6*pow(T9,4.)-1.4987996e10*sqrt(T9)-1.0576906e10*pow(T9,3./2.)+2.7447598e8*pow(T9,5./2.)+7.6425157e7*pow(T9,7./2.)-2.282944e7*pow(T9,-3./2.)/exp(0.050351813/T9);
  else f[16]=1.28039e9;
	
  /* Li6 + n -> a + H3 */	f[17]=pow(T9,-1.5)*2.54e9*exp(-2.39/T9)+(1.-pow(T9/(T9*49.18+1.),1.5)*0.261/pow(T9,1.5))*1.68e8;

  /* Be7 + n -> a + He4 */
  f[18]=(T9*3760.+1.)*20500.;

  /* H2 + p -> g + He3 */	
  if (T9<4.) f[19]=((-15.709674-721.9142*T9+173.23945*T9*T9-77.371692*T9*T9*T9+126.82087e0*pow(T9,1./3.)-206.50853*pow(T9,2./3.)+2120.7339*pow(T9,4./3.)-369.61306*pow(T9,5./3.)+127.8378*pow(T9,7./3.)+100.68769*pow(T9,8./3.))*pow(T9,-2./3.))/exp(1.29042942e0*pow(T9,-1./3.));
  else f[19]=2049.72;
	
  /* H3 + p -> g + He4 */	f[20]=pow(T9,-2./3.)*2.2e4*exp(-3.869/pow(T9,1./3.))*(pow(T9,1./3.)*0.108+1.+pow(T9,2./3.)*1.68+T9*1.26+pow(T9,4./3.)*0.551+pow(T9,5./3.)*1.06);

  /* Li6 + p -> g + Be7 */
  f[21] = 1.25e6*pow(T9,-2./3.)*exp(-8.415/pow(T9,1./3.))*(1.-0.252*T9+5.19e-2*T9*T9-2.92e-3*T9*T9*T9);

  /* Li6 + p -> a + He3 */
  if (T9<2.5) f[22]=((-7.4966212e7-1.9411561e10*T9+1.6262854e10*T9*T9+2.0533495e7*pow(T9,1./3.)+3.9547491e9*pow(T9,2./3.)+3.7907358e10*pow(T9,4./3.)-3.4313768e10*pow(T9,5./3.)-3.9965228e9*pow(T9,7./3.)+4.0333873e8*pow(T9,8./3.))*pow(T9,-2./3.))/exp(4.62619323*pow(T9,-1./3.));
  else f[22]=3.05102e7;

  /* Li7 + p -> a + He4 */
  if (T9<2.5)
    {
      f[23]=((-8.9654123e7-2.5851582e8*T9-2.6831252e7*T9*T9+3.8691673e8*pow(T9,1./3.)+4.9721269e8*pow(T9,2./3.)+2.6444808e7*pow(T9,4./3.)-1.2946419e6*pow(T9,5./3.)-1.0941088e8*pow(T9,7./3.)+9.9899564e7*pow(T9,8./3.))*pow(T9,-2./3.))/exp(7.73389632*pow(T9,-1./3.));
      f[23]+=exp(-1.137519e0*T9*T9-8.6256687*pow(T9,-1./3.))*(3.0014189e7-1.8366119e8*T9+1.7688138e9*T9*T9-8.4772261e9*T9*T9*T9+2.0237351e10*pow(T9,4.)-1.9650068e10*pow(T9,5.)+7.9452762e8*pow(T9,6.)+1.3132468e10*pow(T9,7.)-8.209351e9*pow(T9,8.)-9.1099236e8*pow(T9,9.)+2.7814079e9*pow(T9,10.)-1.0785293e9*pow(T9,11.)+1.3993392e8*pow(T9,12.))*pow(T9,-2./3.);
    }
  else
    {
      f[23]=1.53403e6;
      f[23]+=84516.7;
    }

  /* H2 + a -> g + Li6 */
  f[24]=1.482e1*pow(T9,-2./3.)*exp(-7.435/pow(T9,1./3.))*(1.+6.572*T9+7.6e-2*T9*T9+2.48e-2*T9*T9*T9)+8.28e1*pow(T9,-3./2.)*exp(-7.904/T9);

  /* H3 + a -> g + Li7 */
  if (T9<2.5) f[25]=((0.094614248-4.9273133*T9+99.358965*T9*T9-989.81236*T9*T9*T9+4368.45*pow(T9,4.)+931.93597*pow(T9,5.)-391.07855*pow(T9,6.)+159.23101*pow(T9,7.)-34.407594*pow(T9,8.)+3.3919004*pow(T9,9.)+0.017556217*pow(T9,10.)-0.036253427*pow(T9,11.)+0.0031118827*pow(T9,12.)-0.00008714468*pow(T9,13.))*pow(T9,-1./2.))/(exp(8.4e-7*T9)*pow(1.+1.78616593*T9,3.));
  else f[25]=807.406;

  /* He3 + a -> g + Be7 */
  if(T9<2.5) f[26]=((0.000046165644-0.00046036111*T9-0.021600946*T9*T9+0.069627779*T9*T9*T9+7.346612*pow(T9,4.)-95.123199*pow(T9,5.)+391.13123*pow(T9,6.)-187.23717*pow(T9,7.)+86.111544*pow(T9,8.)-21.630169*pow(T9,9.)+3.6006922*pow(T9,10.)-0.34322836*pow(T9,11.)+0.018106742*pow(T9,12.)-0.00035681506*pow(T9,13.))*pow(T9,-1./2.))/(exp(0.48102949*T9)*pow(1.+1.17917554*T9,3.));
  else f[26]=149.06;

  /* H2 + d -> n + He3 */
  if(T9<4.) f[27]=((-1.8436156e6 - 6.1150115e7*T9 - 2.7251853e7*T9*T9 - 2.2800422e6*T9*T9*T9 - 252433.58*pow(T9,4.) - 284357.41*pow(T9,10./3.) + 906146.25*pow(T9,11./3.) + 1.2270083e7*pow(T9,1./3.) - 1.3680884e7*pow(T9,2./3.) + 1.328894e8*pow(T9,4./3.) - 1.1916242e7*pow(T9,5./3.) + 8.3705218e6*pow(T9,7./3.) + 2.2357751e6*pow(T9,8./3.))*pow(T9,-2./3.))/exp(1.*pow(T9,-1./3.));
  else f[27]=4.98099e7;
	
  /* H2 + d -> p + H3 */
  if(T9<4.) f[28]=((-5.8523126e6+2.3222535e8*T9-9.877862e6*T9*T9+5.2331507e7*pow(T9,1./3.)-1.7022642e8*pow(T9,2./3.)-1.1875268e8*pow(T9,4./3.)+5.2922232e7*pow(T9,5./3.))*pow(T9,-2./3.))/exp(1.0676573*pow(T9,-1./3.));
  else f[28]=4.021e7;

  /* H3 + d -> n + He4 */
  if(T9<2.5) f[29]=6.2265733e8/(exp(0.49711597/T9)*pow(T9,0.56785403)) + exp(-0.23309803*T9*T9 - 1.342742*pow(T9,-1./3.))*(-8.1144927e7 + 2.2315324e9*T9 - 2.9439669e9*T9*T9 + 1.8764462e9*T9*T9 - 6.0511612e8*pow(T9,4.) + 9.5196576e7*pow(T9,5.) - 5.2901086e6*pow(T9,6.))*pow(T9,-2./3.);
  else f[29]=3.40249e8;

  /* He3 + d -> p + He4 */
  if(T9<2.5) f[30]=3.1038385e8/(exp(1.6190981/T9)*pow(T9,0.12159455))+exp(-0.0062340825*T9*T9-1.4540617*pow(T9,-1./3.))*(-3.1335916e7-6.2051071e8*T9-1.8782248e9*T9*T9+6.5642773e8*T9*T9*T9+1.530887e8*pow(T9,4.)-4.9542138e8*pow(T9,10./3.)-1.770285e8*pow(T9,11./3.)+1.14185e8*pow(T9,1./3.)-2.516526e7*pow(T9,13./3.)+1.7500204e8*pow(T9,2./3.)-1.7513362e9*pow(T9,4./3.)+5.2792247e9*pow(T9,5./3.)-3.32382e9*pow(T9,7./3.)+2.0346284e9*pow(T9,8./3.))*pow(T9,-2./3.);
  else f[30]=1.55167e8;

  /* He3 + He3 -> 2p + He4 */
  f[31]=5.59e10*pow(T9,-2./3.)*exp(-12.277/pow(T9,1./3.))*(1.-0.135*T9+2.54e-2*T9*T9 - 1.29e-3*T9*T9*T9);

  /* Li7 + d -> n + a + He4 */
  if (T9<2.5) f[32]=1.66e11*pow(T9,-2./3.)*exp(-10.254/pow(T9,1./3.))+ 1.71e6*pow(T9,-3./2.)*exp(-3.246/T9)  + 1.49e10*pow(T9,-3./2.)*exp(-4.0894/T9)* (2.57e-2/T9 + 2.6314*pow(T9,-2./3.)-4.1929/pow(T9,1./3.) -2.1241+4.1136*pow(T9,1./3.));
  else f[32]=1.37518e9;

  /* Be7 + d -> p + a + He4 */
  f[33]=pow(T9,-2./3.)*1.07e12*exp(-12.428/pow(T9,1./3.));

  /* Li7 + n -> g + Li8 */
  f[34]=3.144e3+4.26e3*pow(T9,-3./2.)*exp(-2.576/T9);

  /* B10 + n -> g + B11 */
  f[35]=66200.;

  /* B11 + n -> g + B12 */
  f[36]=pow(T9,-1.5)*2400.*exp(-0.223/T9)+729.;

  /* C11 + n -> p + B11 */
  f[37]=(1.-sqrt(T9)*0.048+T9*0.01)*1.69e8;

  /* B10 + n -> a + Li7 */
  f[38]=2.2e7*(1.+1.064*T9);
	
  /* Be7 + p -> g + B8 */
  f[39]=2.61e5*pow(T9,-2./3.)*exp(-10.264/pow(T9,1./3.))*(1.-5.11e-2*T9+ 4.68e-2*T9*T9-6.6e-3*T9*T9*T9+3.12e-4*T9*T9*T9*T9)+ 2.05e3*pow(T9,-3./2.)*exp(-7.345/T9);

  /* Be9 + p -> g + B10 */
  f[40]=pow(T9,-2./3.)*1.33e7*exp(-10.359/pow(T9,1./3.)-T9/0.846*T9/0.846)*(pow(T9,1./3.)*0.04+1.+pow(T9,2./3.)*1.52+T9*0.428+pow(T9,4./3.)*2.15+pow(T9,5./3.)*1.54)+pow(T9,-1.5)*96400.*exp(-3.445/T9)+pow(T9,-1.5)*2.72e6*exp(-10.62/T9);

  /* B10 + p -> g + C11 */
  f[41]=pow(T9,-2./3.)*1.68e6*exp(-pow(T9,-1./3.)*12.064)/(pow(pow(T9,2./3.)-0.0273,2.)+4.69e-4)*(1.+0.977*T9+1.87*T9*T9-0.272*T9*T9*T9+
												  0.013*T9*T9*T9*T9); 

  /* B11 + p -> g + C12 */
  f[42]=pow(T9,-2./3.)*4.58e7*exp(-pow(T9,-1./3.)*12.097-pow(T9/0.6,2.))*(1.+0.353*T9-0.842*T9*T9)+pow(T9,-3./2.)*6.82e3*exp(-1.738/T9)+2.8e4*pow(T9,0.104)*exp(-3.892/T9); 
	
  /* C11 + p -> g + N12 */
  f[43]=pow(T9,-2./3.)*42400.*exp(-13.658/pow(T9,1./3.)-T9/1.627*T9/1.627)*(pow(T9,1./3.)*0.031+1.+pow(T9,2./3.)*3.11+T9*0.665+pow(T9,4./3.)*4.61+pow(T9,5./3.)*2.5)+pow(T9,-1.5)*8840.*exp(-7.021/T9);
	
  /* B12 + p -> n + C12 */
  f[44]=pow(T9,-2./3.)*4.02e11*exp(-12.12/pow(T9,1./3.));
	
  /* Be9 + p -> a + Li6 */
  f[45]=2.11e11*pow(T9,-2./3.)*exp(-10.361/pow(T9,1./3.)-pow(T9/0.4,2.))*(1.-0.189*T9+3.52e1*T9*T9)+5.24e8*pow(T9,-3./2.)*exp(-3.446/T9)+4.65e8*exp(-4.396/T9)/(pow(T9,0.293));

  /* B10 + p -> a + Be7 */
  if(T9>0.8) f[46]=1.01e10*pow(T9,-2./3.)*exp(-12.064/pow(T9,1./3.))*(-1.+15.8*T9-2.6*T9*T9+0.125*T9*T9*T9);
  else f[46]= 2.56e10*pow(T9,-2./3.)*exp(-12.064/pow(T9,1./3.))*(1.+5.95*T9+2.92e1*T9*T9-3.16e2*T9*T9*T9+9.14e2*pow(T9,4.)-1.085e3*pow(T9,5.)+4.65e2*pow(T9,6.))/(4.7e-4+pow(pow(T9,2./3.)-2.6e-2,2.));

  /* B12 + p -> a + Be9 */
  f[47]=pow(T9,-2./3.)*2.01e11*exp(-12.12/pow(T9,1./3.));
	
  /* Li6 + a -> g + B10 */
  f[48]=pow(T9,-2./3.)*4.06e6*exp(-18.79/pow(T9,1./3.)-T9/1.326*T9/1.326)*(pow(T9,1./3.)*0.022+1.+pow(T9,2./3.)*1.54+T9*0.239+pow(T9,4./3.)*2.2+pow(T9,5./3.)*0.869)+pow(T9,-1.5)*1910.*exp(-3.484/T9)+10100./T9*exp(-7.269/T9);

  /* Li7 + a -> g + B11 */
  if(T9>1.21) f[49]=1.187e3*pow(T9,-3./2.)*exp(-2.959/T9)+ 7.945e3*(1.+0.1466*T9-1.273e-2*T9*T9)* exp(-4.922/T9)/(pow(T9,2.3e-2));
  else f[49] = 9.72e7*pow(T9,-2./3.)*exp(-19.163/pow(T9,1./3.)-pow(T9/0.4,2.))* (1.+2.84*T9-7.89*T9*T9)+ 3.35e2*pow(T9,-3./2.)*exp(-2.959/T9)+ 1.04e4*exp(-4.922/T9)/(pow(T9,2.3e-2));

  /* Be7 + a -> g + C11 */
  if (T9>2.) f[50] = 1.41e3*exp(-3.015/T9)*pow(T9,0.636);
  else f[50] = 1.29e10*pow(T9,-2./3.)*exp(-23.214/pow(T9,1./3.)-pow(T9/0.8,2.))*(1.-6.47*T9+19.5*T9*T9-19.3*T9*T9*T9)+ 1.25e4*pow(T9,-3./2.)*exp(-6.498/T9)+1.44e5*pow(T9,-3./2.)* exp(-10.177/T9)+1.63e4*exp(-15.281/T9)*pow(T9,0.178);
	
  /* B8 + a -> p + C11 */
  f[51]=pow(T9,-2./3.)*1.08e15*exp(-27.36/pow(T9,1./3.));
	
  /* Li8 + a -> n + B11 */
  f[52]=pow(T9/(T9/15.1+1.),5./6.)*8.62e13*pow(T9,-1.5)*exp(-19.461/pow(T9/(T9/15.1+1.),1./3.));
	
  /* Be9 + a -> n + C12 */
  f[53]=pow(T9,-2./3.)*4.62e13*exp(-23.87/pow(T9,1./3.)-T9/0.049*T9/0.049)*(pow(T9,1./3.)*0.017+1.+pow(T9,2./3.)*8.57+T9*1.05+pow(T9,4./3.)*74.51+pow(T9,5./3.)*23.15)+pow(T9,-1.5)*7.34e-5*exp(-1.184/T9)+pow(T9,-1.5)*0.227*exp(-1.834/T9)+pow(T9,-1.5)*1.26e5*exp(-4.179/T9)+exp(-12.732/T9)*2.4e8;
	
  /* Be9 + d -> n + B10 */
  f[54]=pow(T9,-2./3.)*7.16e8*exp(6.44-12.6/pow(T9,1./3.));
	
  /* B10 + d -> p + B11 */
  f[55]=pow(T9,-2./3.)*9.53e8*exp(7.3-14.8/pow(T9,1./3.));

  /* B11 + d -> n + C12 */
  f[56]=pow(T9,-2./3.)*1.41e9*exp(7.4-14.8/pow(T9,1./3.));

  /* He4 + a + n -> g + Be9 */
  f[57]=2.59e-6/((T9*0.344+1.)*(T9*T9))*exp(-1.062/T9);
	
  /* He4 + 2a -> g + C12 */
  f[58]=pow(T9,-1.5)*2.79e-8*pow(T9,-1.5)*exp(-4.4027/T9)+pow(T9,-1.5)*1.35e-8*exp(-24.811/T9);
	
  /* Li8 + p -> n + a + He4 */
  f[59]=pow(T9,-2./3.)*8.65e9*exp(-8.52/pow(T9,1./3.)-T9/2.53*T9/2.53)+pow(T9,-1.5)*2.31e9*exp(-4.64/T9);
	
  /* B8 + n -> p + a + He4 */
  f[60]=4.02e8;
	
  /* Be9 + p -> d + a + He4 */
  f[61]=pow(T9,-2./3.)*2.11e11*exp(-10.359/pow(T9,1./3.)-T9/0.52*T9/0.52)*(pow(T9,1./3.)*0.04+1.+pow(T9,2./3.)*1.09+T9*0.307+pow(T9,4./3.)*3.21+pow(T9,5./3.)*2.3)+5.79e8/T9*exp(-3.046/T9)+pow(T9,-0.75)*8.5e8*exp(-5.8/T9);
	
  /* B11 + p -> 2 a + He4 */
  f[62]=pow(T9,-2./3.)*2.2e12*exp(-12.095/pow(T9,1./3.)-T9/1.644*T9/1.644)*(pow(T9,1./3.)*0.034+1.+pow(T9,2./3.)*0.14+T9*0.034+pow(T9,4./3.)*0.19+pow(T9,5./3.)*0.116)+pow(T9,-1.5)*4.03e6*exp(-1.734/T9)+pow(T9,-1.5)*6.73e9*exp(-6.262/T9)+3.88e9/T9*exp(-14.154/T9);

  /* C11 + n -> 2a + He4 */
  f[63]=1.58e8;

  /* C12 + n -> g + C13 */
  f[64]=450.;

  /* C13 + n -> g + C14 */
  f[65]=pow(T9,-1.5)*2.38e5*exp(-1.67/T9)+119.;

  /* N14 + n -> g + N15 */
  f[66]=9940.;

  /* N13 + n -> p + C13 */
  f[67]= 1.178e8*(1.+3.36e-1*T9-3.792e-2*T9*T9+2.02e-3*T9*T9*T9);
  f[67]*=(1.+1.131*exp(-1.2892e1/T9+1.9e-2*T9));

  /* N14 + n -> p + C14 */
  f[68]=(sqrt(T9)*0.361+1.+T9*0.502)*2.39e5+1.112e8/sqrt(T9)*exp(-4.983/T9);

  /* O15 + n -> p + N15 */	
  f[69] = 1.158e8*(1.+2.19e-1*T9-2.9e-2*T9*T9+1.73e-3*T9*T9*T9);
  f[69]*=(1.+3.87e-1*exp(-26.171/T9+1.18e-1*T9));

  /* O15 + n -> a + C12 */
  f[70]=(sqrt(T9)*0.188+1.+T9*0.015)*3.5e7;
	
  /* C12 + p -> g + N13 */
  f[71]=2.e7*pow(T9,-2./3.)*exp(-13.692*pow(T9,-1./3.)-pow(T9/0.46,2.))*(1.+9.89*T9-59.8*T9*T9+266.*T9*T9*T9)+1.e5*pow(T9,-3./2.)*exp(-4.913/T9)+4.24e5*pow(T9,-3./2.)*exp(-21.62/T9);
	
  /* C13 + p -> g + N14 */
  f[72]=9.57e7*pow(T9,-2./3.)*exp(-13.720*pow(T9,-1./3.)-T9*T9)*(1.+3.56*T9)+1.5e6*pow(T9,-3./2.)*exp(-5.930/T9)+6.83e5*pow(T9,-8.64e-1)*exp(-12.057/T9);
  f[72]*=(1.-2.07*exp(-37.938/T9));
	
  /* C14 + p -> g + N15 */
  f[73]=pow(T9,-2./3.)*6.8e6*exp(-13.741/pow(T9,1./3.)-T9/5.721*T9/5.721)*(pow(T9,1./3.)*0.03+1.+pow(T9,2./3.)*0.503+T9*0.107+pow(T9,4./3.)*0.213+pow(T9,5./3.)*0.115)+pow(T9,-1.5)*5360.*exp(-3.811/T9)+pow(T9,-1./3.)*98200.*exp(-4.739/T9);
	
  /* N13 + p -> g + O14 */
  f[74]=pow(T9,-2./3.)*4.04e7*exp(-15.202/pow(T9,1./3.)-T9/1.191*T9/1.191)*(pow(T9,1./3.)*0.027+1.-pow(T9,2./3.)*0.803-T9*0.154+pow(T9,4./3.)*5.+pow(T9,5./3.)*2.44)+pow(T9,-1.5)*2.43e5*exp(-6.348/T9);
	
  /* N14 + p -> g + O15 */
  f[75]=pow(T9,-2./3.)*4.9e7*exp(-15.228/pow(T9,1./3.)-T9/3.294*T9/3.294)*(pow(T9,1./3.)*0.027+1.-pow(T9,2./3.)*0.778-T9*0.149+pow(T9,4./3.)*0.261+pow(T9,5./3.)*0.127)+pow(T9,-1.5)*2370.*exp(-3.011/T9)+exp(-12.53/T9)*21900.;
	
  /* N15 + p -> g + O16 */
  f[76]=pow(T9,-2./3.)*9.78e8*exp(-15.251/pow(T9,1./3.)-T9/0.45*T9/0.45)*(pow(T9,1./3.)*0.027+1.+pow(T9,2./3.)*0.219+T9*0.042+pow(T9,4./3.)*6.83+pow(T9,5./3.)*3.32)+pow(T9,-1.5)*11100.*exp(-3.328/T9)+pow(T9,-1.5)*14900.*exp(-4.665/T9)+pow(T9,-1.5)*3.8e6*exp(-11.048/T9);
	
  /* N15 + p -> a + C12 */
  if (T9>2.5) f[77]= 4.17e7*pow(T9,0.917)*exp(-3.292/T9);
  else f[77]= 1.12e12*pow(T9,-2./3.)*exp(-15.253*pow(T9,-1./3.)-pow(T9/0.28,2.))*(1.+4.95*T9+143.*T9*T9)+1.01e8*pow(T9,-3./2.)* exp(-3.643/T9)+1.19e9*pow(T9,-3./2.)*exp(-7.406/T9);
	
  /* C12 + a -> g + O16 */
  f[78]=1.04e8/(T9*T9)*exp(-32.12/pow(T9,1./3.)-T9/3.496*T9/3.496)/pow(pow(T9,-2./3.)*0.0489+1.,2.)+1.76e8/(T9*T9)/pow(pow(T9,-2./3.)*0.2654+1.,2.)*exp(-32.12/pow(T9,1./3.))+pow(T9,-1.5)*1250.*exp(-27.499/T9)+pow(T9,5.)*0.0143*exp(-15.541/T9);
	
  /* B10 + a -> p + C13 */
  f[79]=pow(T9,-2./3.)*9.6e14*exp(-27.99/pow(T9,1./3.));
	
  /* B11 + a -> p + C14 */
  f[80]=pow(T9,-2./3.)*5.37e11*exp(-28.234/pow(T9,1./3.)-T9/0.347*T9/0.347)*(pow(T9,1./3.)*0.015+1.+pow(T9,2./3.)*5.575+T9*0.576+pow(T9,4./3.)*15.888+pow(T9,5./3.)*4.174)+pow(T9,-1.5)*0.00544*exp(-2.827/T9)+pow(T9,-1.5)*336.*exp(-5.178/T9)+5.32e6/pow(T9,0.375)*exp(-11.617/T9);
	
  /* C11 + a -> p + N14 */
  f[81]=pow(T9/(T9*0.0478+1.+pow(T9,5./3.)*0.00756/pow(T9*0.0478+1.,2./3.)),5./6.)*7.15e15*pow(T9,-1.5)*exp(-31.883/pow(T9/(T9*0.0478+1.+pow(T9,5./3.)*0.00756/pow(T9*0.0478+1.,2./3.)),1./3.));
  f[81]*= (1.+0.140*exp(-0.275/T9-0.210*T9));

  /* N12 + a -> p + O15 */
  f[82]=pow(T9,-2./3.)*5.59e16*exp(-35.6/pow(T9,1./3.));
	
  /* N13 + a -> p + O16 */
  f[83]=pow(T9/(T9*0.0776+1.+pow(T9,5./3.)*0.0264/pow(T9*0.0776+1.,2./3.)),5./6.)*3.23e17*pow(T9,-1.5)*exp(-35.829/pow(T9/(T9*0.0776+1.+pow(T9,5./3.)*0.0264/pow(T9*0.0776+1.,2./3.)),1./3.));
	
  /* B10 + a -> n + N13 */
  f[84]=pow(T9,-2./3.)*1.2e13*exp(-27.989/pow(T9,1./3.)-T9/9.589*T9/9.589);

  /* B11 + a -> n + N14 */
  f[85]=pow(T9,-2./3.)*6.97e12*exp(-28.234/pow(T9,1./3.)-T9/0.14*T9/0.14)*(pow(T9,1./3.)*0.015+1.+pow(T9,2./3.)*8.115+T9*0.838+pow(T9,4./3.)*39.804+pow(T9,5./3.)*10.456)+pow(T9,-1.5)*1.79*exp(-2.827/T9)+pow(T9,-1.5)*1710.*exp(-5.178/T9)+pow(T9,0.6)*4.49e6*exp(-8.596/T9);

  /* B12 + a -> n + N15 */
  f[86]=pow(T9,-2./3.)*3.04e15*exp(-28.45/pow(T9,1./3.));

  /* C13 + a -> n + O16 */
  if(T9>4.) f[87]= 7.59e6*pow(T9,1.078)*exp(-12.056/T9);
  else f[87]= 3.78e14/T9/T9*exp(-32.333*pow(T9,-1./3.)-pow(T9/0.71,2.))*(1.+4.68e1*T9-2.92e2*T9*T9+7.38e2*T9*T9*T9)+2.3e7*pow(T9,0.45)*exp(-13.03/T9);
  f[87]*=(1.+7.3318e1*exp(-58.176/T9-1.98e-1*T9));
	
  /*Check to ensure positive*/
  for(ie=11;ie<=87;ie++) f[ie]=fabs(f[ie]);
		
  return;
}

void reactionRates(double T9,double *f,double *r){
  rate_weak(f,r);
  rate_pn(T9,f,r);
  rate_all(T9,f);
}
